    "src/main.ts",
    "src/environments/environment.ts",
    "src/environments/environment.prod.ts",
    "src/remotes.d.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "exclude": [
    "jest.config.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.ts"
  ]
}
